{
	"name": "middle.messenger.praktikum.yandex",
	"version": "1.0.0",
	"description": "Проект доступен по адресу: https://amazing-tereshkova-cec837.netlify.app",
	"main": "server.js",
	"dependencies": {
		"express": "^4.17.1",
		"pug-cli": "^1.0.0-alpha6",
		"scss": "^0.2.4",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.21.0",
		"@typescript-eslint/parser": "^4.21.0",
		"cssnano": "^4.1.10",
		"eslint": "^7.22.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.1",
		"husky": "^5.2.0",
		"lint-staged": "^10.5.4",
		"parcel-bundler": "^1.12.5",
		"prettier": "^2.2.1",
		"pug": "^3.0.2",
		"sass": "^1.32.8",
		"stylelint": "^13.12.0",
		"stylelint-config-dvpnt": "^1.4.0",
		"typescript": "^4.2.4"
	},
	"scripts": {
		"clientBuild": "pug -O '{module: true}' -c --no-debug --name-after-file -o dist/templates static/components/*/*.template.pug && pug -O '{module: true}' -c --no-debug --name-after-file -o dist/pages static/pages/*/*.template.pug",
		"cleanup": "rm -rf .cache dist",
		"test": "echo \"Error: no test specified\" && exit 1",
		"parcelStart": "parcel -p 3001 static/index.pug static/pages/* --no-cache",
		"parcelBuild": "parcel build static/index.pug static/pages/*/index.ts",
		"start": "npm run clientBuild && npm run parcelBuild && node server.js",
		"lint": "lint-staged",
		"lintJsFix": "eslint --ext js, ts --cache --fix -- './'",
		"lintSCSSFix": "stylelint --syntax scss --fix -- './**.scss'",
		"lintFix": "nrun lintJsFix && nrun lintSCSSFix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Gontareva/middle.messenger.praktikum.yandex.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Gontareva/middle.messenger.praktikum.yandex/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --quiet"
		}
	},
	"lint-staged": {
		"*.js,*.ts": "eslint --quiet",
		"*.scss": "stylelint --syntax scss"
	},
	"homepage": "https://github.com/Gontareva/middle.messenger.praktikum.yandex#readme",
	"keywords": []
}
